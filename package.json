{
  "name": "express-chat",
  "version": "1.0.0",
  "description": "Sample Express app for chat application using socket.io",
  "main": "dist",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "prestart": "npm run -s build",
    "dev": "SET NODE_ENV=development && nodemon -w src --exec \"babel-node src/index --presets env,stage-0\"",
    "start": "npm run -s build && SET NODE_ENV=production && nodemon start dist"
  },
  "dependencies": {
    "express": "^4.16.4",
    "socket.io": "^2.1.1",
    "nodemon": "^1.18.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1"
  },
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1"
  },
  "author": "CMR",
  "license": "MIT"
}
